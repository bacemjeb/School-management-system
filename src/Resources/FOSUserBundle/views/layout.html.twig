<html lang="en" >
    <!-- begin::Head -->
    <head>
        <meta charset="utf-8" />
        {% block title %}
            <title>
                E-electricity
            </title>
        {% endblock %}
        <meta name="description" content="Latest updates and statistic charts">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <!--begin::Web font -->
        {% block stylesheets %}
            <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
            <script>
                WebFont.load({
                    google: {"families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"]},
                    active: function () {
                        sessionStorage.fonts = true;
                    }
                });
            </script>
            <!--end::Web font -->
            <!--begin::Base Styles -->  
            <link href="{{ asset('build/custom/style.css') }}" rel="stylesheet" type="text/css" />
            <!--begin::Page Vendors -->
            <link href="{{ asset('build/vendors/custom/fullcalendar/fullcalendar.bundle.css') }}" rel="stylesheet" type="text/css" />
            <!--end::Page Vendors -->
            <link href="{{ asset('build/vendors/base/vendors.bundle.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('build/demo/default/base/style.bundle.css') }}" rel="stylesheet" type="text/css" />
            <!--end::Base Styles -->
            <link rel="shortcut icon" href="{{ asset('build/demo/default/media/img/logo/favicon.ico') }}" />
        {% endblock %}
    </head>
    <!-- end::Head -->
    <!-- end::Body -->
    <body  class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-dark m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default"  >
        <!-- begin:: Page -->
        <div class="m-grid m-grid--hor m-grid--root m-page">
            <!-- BEGIN: Header -->
            
            <!-- END: Header -->
            <!-- begin::Body -->
            <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop body-dashbord">
                <!-- BEGIN: Left Aside -->
                <button class="m-aside-left-close  m-aside-left-close--skin-dark " id="m_aside_left_close_btn">
                    <i class="la la-close"></i>
                </button>
              
                    <!-- END: Aside Menu -->
                </div>
                <!-- END: Left Aside -->
                <div class="m-grid__item m-grid__item--fluid m-wrapper">
                    <!-- BEGIN: Subheader -->
                    <div class="m-subheader ">
                        <div class="d-flex align-items-center">
                            <div class="mr-auto">
                                <h3 class="m-subheader__title ">
                                    {% block breadcrumb %}
                                    {% endblock %}
                                </h3>
                            </div>
                            {% block button %}
                            {% endblock %}  
                            <div>

                            </div>
                        </div>
                    </div>
                    <!-- END: Subheader -->

                   <div class="m-section__content block-content">
                                                {% set _flashMessages = app.session.flashbag.all() %}
                             {%if _flashMessages%}
                        <div class="m-alert m-alert--outline alert alert-success alert-dismissible fade show">
                            {% for type, messages in _flashMessages %}
                                {% for flashMessage in messages %}
                                    <div class="alert{% if type !='message' %} alert-{{ type }}{% endif %} alert-dismissable">
                                        <button aria-hidden="true" data-dismiss="alert" class="close" type="button"></button>
                                        {{ flashMessage|raw }}
                                    </div>
                                {% endfor %}
                            {% endfor %}
                            {%endif%}
                        </div>
                        <!--BEGIN::Section-->
                        {% block fos_user_content %}
                        {% endblock %}
                        <!--End::Section-->
                    </div>
                </div>
            </div>
            <!-- end:: Body -->
            <!-- begin::Footer -->

            <!-- end::Footer -->
        </div>
        <!-- end:: Page -->
        <!-- begin::Quick Sidebar -->

        <!-- end::Quick Sidebar -->
        <!-- begin::Scroll Top -->
        <div id="m_scroll_top" class="m-scroll-top">
            <i class="la la-arrow-up"></i>
        </div>
        <!-- end::Scroll Top -->		    <!-- begin::Quick Nav -->
        {% block js %}
            <!-- begin::Quick Nav -->	
            <!--begin::Base Scripts -->
            <script src="{{ asset('build/vendors/base/vendors.bundle.js') }}" type="text/javascript"></script>
            <script src="{{ asset('build/demo/default/base/scripts.bundle.js') }}" type="text/javascript"></script>
            <!--end::Base Scripts -->   
            <!--begin::Page Vendors -->
            <script src="{{ asset('build/vendors/custom/fullcalendar/fullcalendar.bundle.js') }}" type="text/javascript"></script>
            <!--end::Page Vendors -->  
            <!--begin::Page Snippets -->
            <script src="{{ asset('build/app/js/dashboard.js') }}" type="text/javascript"></script>
            <!--end::Page Snippets -->
        {% endblock %}
    </body>
    <!-- end::Body -->
</html>

